<%= render_inventory_item(@latest_inventory_item)  %>
<%#= @latest_inventory_item.inspect %>

<table border='1' >
    <tr>
        <th>Name
        <th>AssetType
        <th>Ancestors
  <% @inventory_items.each do |x| %>
    <tr>
        <td><%= link_to x.name, x %>:
        <td><%= x.asset_type %>
        <td><%= x.ancestors %>
    <% end %>
</table>